{
  "expo": {
    "name": "react-native-learn",
    "slug": "react-native-learn",
    "version": "1.0.0",
    "scheme": "aora",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.hans",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow HANS to use Face ID."
      },
      "bundleIdentifier": "com.jsm.hans"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.jsm.hans"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow HANS to use Face ID."
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow HANS to use Face ID."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fb9a8b1d-ebbe-4dee-ba9c-445dda6a3f9c"
      }
    }
  }
}
